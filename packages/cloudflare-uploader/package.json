{
  "name": "@ai-newscast/cloudflare-uploader",
  "version": "1.0.0",
  "description": "Cloudflare R2 uploader for newscast files with S3-compatible API",
  "type": "module",
  "main": "./uploader.ts",
  "bin": {
    "cloudflare-uploader": "./command.ts"
  },
  "exports": {
    ".": "./uploader.ts",
    "./command": "./command.ts",
    "./types": "./types.ts"
  },
  "scripts": {
    "dev": "node --experimental-strip-types command.ts",
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "upload": "node --experimental-strip-types command.ts upload",
    "test": "node --experimental-strip-types command.ts upload --test"
  },
  "dependencies": {
    "@ai-newscast/core": "workspace:*",
    "@aws-sdk/client-s3": "^3.709.0",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "cloudflare",
    "r2",
    "s3",
    "uploader",
    "storage",
    "newscast",
    "ai"
  ],
  "authors": [
    "AI Newscast Team"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  }
}