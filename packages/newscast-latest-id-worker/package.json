{
  "name": "@ai-newscast/newscast-latest-id-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker for managing latest newscast ID with KV storage",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node build.ts",
    "dev": "wrangler dev",
    "deploy": "pnpm build && wrangler deploy",
    "tail": "wrangler tail",
    "type-check": "tsc --noEmit",
    "update:local": "wrangler kv key put \"latest-newscast-id\" --namespace-id 1a002997dc124ce9a4ff5080a7e2b5e6 --local",
    "update:remote": "wrangler kv key put \"latest-newscast-id\" --namespace-id 1a002997dc124ce9a4ff5080a7e2b5e6 --remote",
    "get:local": "wrangler kv key get \"latest-newscast-id\" --namespace-id 1a002997dc124ce9a4ff5080a7e2b5e6 --local",
    "get:remote": "wrangler kv key get \"latest-newscast-id\" --namespace-id 1a002997dc124ce9a4ff5080a7e2b5e6 --remote"
  },
  "dependencies": {},
  "devDependencies": {
    "wrangler": "^4.43.0",
    "@types/node": "^24.7.2",
    "@cloudflare/workers-types": "^4.20251011.0",
    "esbuild": "^0.25.11",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "kv",
    "newscast",
    "api"
  ],
  "authors": [
    "AI Newscast Team"
  ],
  "license": "MIT"
}